{
    "name": "fresh/firebase-cloud-messaging-bundle",
    "description": "Provides integration with Firebase Cloud Messaging API.",
    "keywords": ["symfony", "bundle", "fcm", "firebase", "cloud", "messaging", "api client", "firebase cloud messaging", "notification", "push", "push-notification", "web-push", "web-push-notification", "android", "ios"],
    "homepage": "https://firebase.google.com/docs/cloud-messaging/",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Artem Genvald",
            "email": "genvaldartem@gmail.com",
            "role": "Creator"
        }
    ],
    "support": {
        "email": "genvaldartem@gmail.com",
        "issues": "https://github.com/fre5h/FirebaseCloudMessagingBundle/issues"
    },
    "require": {
        "php": ">=5.6.0",
        "symfony/config": "^2.8.6 || ^3.0.6",
        "symfony/event-dispatcher": "^2.8.6 || ^3.0.6",
        "symfony/framework-bundle": "^2.8.6 || ^3.0.6",
        "fresh/firebase-cloud-messaging-api-php-client": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.7",
        "squizlabs/php_codesniffer": "~2.7",
        "escapestudios/symfony2-coding-standard": "~2.9",
        "matthiasnoback/symfony-config-test": "2.1.*"
    },
    "autoload": {
        "psr-4": {
            "Fresh\\FirebaseCloudMessagingBundle\\": ""
        },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Fresh\\FirebaseCloudMessagingBundle\\Tests\\": "Tests/"
        }
    }
}
