{
    "name": "fresh/firebase-cloud-messaging-bundle",
    "description": "Provides integration with Firebase Cloud Messaging API.",
    "keywords": ["symfony", "bundle", "fcm", "firebase", "cloud", "messaging", "api client", "firebase cloud messaging", "notification", "push", "push-notification", "web-push", "web-push-notification", "android", "ios"],
    "homepage": "https://firebase.google.com/docs/cloud-messaging/",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Artem Henvald",
            "email": "genvaldartem@gmail.com",
            "role": "Creator"
        },
        {
            "name": "Community",
            "homepage": "https://github.com/fre5h/FirebaseCloudMessagingBundle/graphs/contributors",
            "role": "Contributors"
        }
    ],
    "support": {
        "email": "genvaldartem@gmail.com",
        "issues": "https://github.com/fre5h/FirebaseCloudMessagingBundle/issues"
    },
    "require": {
        "php": "^7.3.8",
        "guzzlehttp/guzzle": "^6.3",
        "symfony/config": "^4.4 || ^5.0",
        "symfony/event-dispatcher": "^4.4 || ^5.0",
        "symfony/framework-bundle": "^4.4 || ^5.0",
        "symfony/http-foundation": "^4.4 || ^5.0",
        "symfony/validator": "^4.4 || ^5.0"
    },
    "require-dev": {
        "escapestudios/symfony2-coding-standard": "^3.11",
        "johnkary/phpunit-speedtrap": "^3.2",
        "phpstan/phpstan": "^0.12",
        "phpstan/phpstan-deprecation-rules": "^0.12",
        "phpstan/phpstan-phpunit": "^0.12",
        "phpstan/phpstan-symfony": "^0.12",
        "phpunit/phpunit": "^9.1",
        "slam/phpstan-extensions": "^4.0"
    },
    "autoload": {
        "psr-4": {
            "Fresh\\FirebaseCloudMessagingBundle\\": ""
        },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Fresh\\FirebaseCloudMessagingBundle\\Tests\\": "Tests/"
        }
    }
}
